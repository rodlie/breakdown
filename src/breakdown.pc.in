Name: Breakdown
Description: A library for parsing Google Breakpad crash dumps
URL: https://github.com/rodlie/breakdown
Version: @PROJECT_VERSION@
Requires: libzip
Cflags: -I"${includedir}"
Libs: -L"${libdir}" -lBreakdown
